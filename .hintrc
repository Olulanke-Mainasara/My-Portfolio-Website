{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "width: fit-content",
          "height: fit-content",
          "backdrop-filter"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "img[loading]"
        ]
      }
    ]
  }
}